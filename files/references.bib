  langid = {english},
  keywords = {Group identity,Mathematical ability,Psychology of women,Self-confidence,Self-esteem,Self-perception,Short-term memory,Social consciousness,Social psychology,Social types,Stereotypes},
  annotation = {40 citations (Crossref) [2024-06-23]},
