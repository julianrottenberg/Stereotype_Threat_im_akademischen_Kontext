@article{shenoudaEffectsGenderStereotypes2014,
  title = {Effects of Gender Stereotypes and Stereotype Threat on Children's Performance on a Spatial Task},
  author = {Shenouda, Christine K. and Danovitch, Judith H.},
  year = {2014},
  journal = {Effects of gender stereotypes and stereotype threat on children's performance on a spatial task},
  volume = {27},
  urldate = {2015-03-11},
  abstract = {Although boys and girls start out performing similarly on science, technology engineering, and mathematics (STEM)-related school subjects, the gap between them widens as they mature, leading to the underrepresentation of women in STEM fields at the college level and in the workforce. One explanation for this phenomenon is stereotype threat: that females are intimidated by the persistent negative stereotypes about women in STEM fields. Examining the inception and development of stereotype threat among young children is crucial for understanding and bridging the STEM gender gap. Two experiments investigated the effect of stereotype threat on performance on a spatial task among children ages 4 to 10. After a gender identity activation manipulation, children replicated a series of designs with LEGO blocks. In Experiment 1 (N = 22), 4-yearold girls whose gender identity was activated were significantly slower than girls whose gender identity was not activated. In Experiment 2 (N = 160), boys and girls in kinder-garten through third grade did not perform differently in stereotype-threat conditions or a condition where gender was not activated. However, girls were consistently slower at the block-construction task than boys. In addition, older children's speed was predicted by their stereotypical attitudes towards LEGO blocks, whether their favorite toy was LEGO blocks, and how frequently they played with blocks at home. Results suggest that gender stereotypes affect children's performance on a gendered activity as early as age 4 and may be continuously affecting older girls' performance. Implications for understanding precursors of the STEM gender gap are discussed.},
  keywords = {No DOI found},
  file = {/home/julian/Zotero/storage/HSDNZ962/Shenouda_Danovitch_2014_Effects of gender stereotypes and stereotype threat on children's performance.pdf}
}

@article{shermanMathematicalAnxietyEtiology2020,
  title = {Mathematical Anxiety: Etiology, Development and Links with Mathematical Achievement},
  author = {Sherman, Brian F. and Wither, David P.},
  year = {2020 JAN-FEB},
  journal = {Mathematics Education Research Journal},
  volume = {15},
  number = {2},
  pages = {138--150},
  issn = {1033-2170, 2211-050X},
  doi = {10.1007/BF03217375},
  urldate = {2024-06-23},
  abstract = {Mathematical anxiety, one of the types of emotional reactions associated with learning. The phenomenon is negatively associated with mathematical abilities and the level of achievement in mathematics. Studies show that up to 48\% of schoolchildren have a high level of mathematical anxiety. Recently, the number of studies of maths anxiety has increased, which requires a general review of results. This review analyzes the most significant current research on mathematical anxiety. The key role of working memory in the functioning of anxiety is shown and a number of effects associated with anxiety are discussed: "anxiety-complexity" effect, the avoidance effect, and the split effect. Brain mechanisms of maths anxiety and its interaction with cognitive abilities are discussed. Studies have shown that mathematical anxiety is associated with activity in areas of the brain associated with experiencing physical pain, and affects the numerical processing. The genetic and environmental etiology of the phenomenon is shown: up to 70\% of individual differences in maths anxiety are explained by environmental factors. The article describes family, school, and social factors that contribute to the development of the phenomena. The paper presents theoretical ideas and the results of experimental studies of the connections of the construct with other psychological processes: motivation, self-concept, etc. The paper also discusses the positive effects of anxiety, e.g. arousal that can improve one's performance. The article analyzes current methods used for reducing effects of mathematical anxiety: expressive writing, meditation, change of attitude regarding maths anxiety. The review highlights the need for further investigations of maths anxiety and its mechanisms.},
  copyright = {http://www.springer.com/tdm},
  langid = {english},
  annotation = {22 citations (Crossref) [2024-06-23]},
  file = {/home/julian/Zotero/storage/7HG78EI7/Budakova et al_2020_Mathematical anxiety.pdf}
}

@article{shewachStereotypeThreatEffects2019,
  title = {Stereotype Threat Effects in Settings with Features Likely versus Unlikely in Operational Test Settings: A Meta-Analysis.},
  shorttitle = {Stereotype Threat Effects in Settings with Features Likely versus Unlikely in Operational Test Settings},
  author = {Shewach, Oren R. and Sackett, Paul R. and Quint, Sander},
