@article{allenThreateningHeartMind2016,
  title = {Threatening the Heart and Mind of Gender Stereotypes: {{Can}} Imagined Contact Influence the Physiology of Stereotype Threat?},
  shorttitle = {Threatening the Heart and Mind of Gender Stereotypes},
  author = {Allen, Ben},
  year = {2016},
  month = jan,
