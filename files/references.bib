  author = {Bullock, Justin L. and Lai, Cindy J. and Lockspeiser, Tai and O'Sullivan, Patricia S. and Aronowitz, Paul and Dellmore, Deborah and Fung, Cha-Chi and Knight, Christopher and Hauer, Karen E.},
