  keywords = {Attention,Bias (Epidemiology),EEG,Feedback,MATHEMATICS problems & exercises,NEUROSCIENCES,Phase locking,PHYSIOLOGICAL stress,PREFRONTAL cortex,PSYCHOLOGICAL feedback,SHORT-term memory,SOCIAL aspects,Social neuroscience,Stereotype threat,STEREOTYPES,Stereotyping,Time-frequency},
