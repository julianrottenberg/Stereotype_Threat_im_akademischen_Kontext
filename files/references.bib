  file = {/home/julian/Zotero/storage/K7DZ4MWD/Bedyńska et al_2021_Measuring Stereotype Threat at Math and Language Arts in Secondary School.pdf}
}

@article{bedynskaStereotypeThreatAntecedent2020,
  title = {Stereotype Threat as an Antecedent to Domain Identification and Achievement in Language Arts in Boys: A Cross-Sectional Study},
  shorttitle = {Stereotype Threat as an Antecedent to Domain Identification and Achievement in Language Arts in Boys},
  author = {Bedy{\'n}ska, Sylwia and Krejtz, Izabela and Rycielski, Piotr and Sedek, Grzegorz},
