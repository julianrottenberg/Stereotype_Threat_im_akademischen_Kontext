@article{bullockMovingMasteryChanges2022,
  title = {Moving toward Mastery: {{Changes}} in Student Perceptions of Clerkship Assessment with Pass/Fail Grading and Enhanced Feedback},
  shorttitle = {Moving toward Mastery},
  author = {Bullock, Justin L. and Seligman, Lee and Lai, Cindy J. and O'Sullivan, Patricia S. and Hauer, Karen E.},
  year = {2022},
