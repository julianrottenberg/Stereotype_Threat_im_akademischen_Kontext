@article{mazerolleHighlightingReducingImpact2020,
  title = {Highlighting and Reducing the Impact of Negative Aging Stereotypes during Older Adults' Cognitive Testing},
  author = {Mazerolle, Marie and R{\'e}gner, Isabelle and Rigalleau, Fran{\c c}ois and Huguet, Pascal},
