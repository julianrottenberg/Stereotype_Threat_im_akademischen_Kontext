  langid = {english},
  keywords = {Attention,Attitude (Psychology),Golf techniques,Golfers,Information processing,Memory,Putting (Golf),Short-term memory,Social psychology,Stereotypes,Stereotypes in sports},
  annotation = {129 citations (Crossref) [2024-06-23]},
  file = {/home/julian/Zotero/storage/35J76Z8I/Beilock et al_2006_On the Causal Mechanisms of Stereotype Threat.pdf;/home/julian/Zotero/storage/UK59C6IB/Beilock et al_2006_On the causal mechanisms of stereotype threat.pdf}
