}

@article{kishidaImplicitSignalsSmall2012a,
  title = {Implicit Signals in Small Group Settings and Their Impact on the Expression of Cognitive Capacity and Associated Brain Responses.},
  author = {Kishida, Kenneth T. and Yang, Dongni and Quartz, Karen Hunter and Quartz, Steven R. and Montague, P. Read},
