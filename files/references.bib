  file = {/home/julian/Zotero/storage/9DAUQCVM/Kostovičová_2019_The differential effects of good luck belief on cognitive performance in boys.pdf}
}

@article{koveraEyewitnessIdentificationIts2021a,
  title = {Eyewitness Identification in Its Social Context.},
  author = {Kovera, Margaret Bull and Evelo, Andrew J.},
  year = {2021},
  month = sep,
  journal = {Journal of Applied Research in Memory and Cognition},
