@article{woutManyFacesStereotype2008,
  title = {The Many Faces of Stereotype Threat: {{Group-}} and Self-Threat},
  shorttitle = {The Many Faces of Stereotype Threat},
  author = {Wout, Daryl and Danso, Henry and Jackson, James and Spencer, Steve},
  year = {2008},
