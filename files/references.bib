}

@article{breletEnhancingOlderDrivers2016a,
  title = {Enhancing Older Drivers' Safety: {{On}} Effects Induced by Stereotype Threat to Older Adults' Driving Performance, Working Memory and Self-Regulation.},
  shorttitle = {Enhancing {{Older Drivers}}' {{Safety}}},
  author = {Brelet, Lisa and Mot{\'a}k, Ladislav and Ginet, Magali and Huet, Nathalie and Izaute, Marie and Gabaude, Catherine},
