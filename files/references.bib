@article{hsuInfluencesStereotypeThreat2022,
  title = {Influences of Stereotype Threat on the Mathematics Performance of High School Athletes},
  author = {Hsu, Yawen and Li, Yinru},
  year = {2022},
  month = jun,
  journal = {Current Psychology},
  volume = {41},
