  author = {Leanos, Shirley and K{\"u}r{\"u}m, Esra and {Strickland-Hughes}, Carla M and Ditta, Annie S and Nguyen, Gianhu and Felix, Miranda and Yum, Hara and Rebok, George W and Wu, Rachel},
