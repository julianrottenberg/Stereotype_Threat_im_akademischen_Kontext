  title = {Stereotype Threat in Learning Situations? {{An}} Investigation among Language Minority Students},
  shorttitle = {Stereotype Threat in Learning Situations?},
  author = {K{\"o}nig, Sabrina and {Stang-Rabrig}, Justine and Hannover, Bettina and Zander, Lysann and McElvany, Nele},
