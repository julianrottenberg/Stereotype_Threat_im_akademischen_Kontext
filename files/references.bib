  keywords = {Consumer Satisfaction,Costs and Cost Analysis,Gender,Human Sex Differences,Information processing,Pricing,Quality,Retailing,Role Perception,Social groups,Social Groups,Stereotyped Attitudes,Stereotypes,Threat},
