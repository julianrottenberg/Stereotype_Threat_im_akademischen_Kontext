@article{bertramsSelfcontrolCapacityModerates2022,
  title = {Self-Control Capacity Moderates the Effect of Stereotype Threat on Female University Students' Worry during a Math Performance Situation},
  author = {Bertrams, Alex and Lindner, Christoph and Muntoni, Francesca and Retelsdorf, Jan},
  year = {2022},
