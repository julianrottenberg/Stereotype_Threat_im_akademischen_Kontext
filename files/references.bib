@article{hayEffectivenessTwoCognitive2016a,
  title = {Effectiveness of Two Cognitive Training Programs on the Performance of Older Drivers with a Cognitive Self-Assessment Bias},
  author = {Hay, Marion and Adam, Nicolas and Bocca, Marie-Laure and Gabaude, Catherine},
  year = {2016},
  month = sep,
  journal = {European Transport Research Review},
  volume = {8},
