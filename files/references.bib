@article{chrastilActivePassiveSpatial2013a,
  title = {Active and Passive Spatial Learning in Human Navigation: {{Acquisition}} of Survey Knowledge.},
  shorttitle = {Active and Passive Spatial Learning in Human Navigation},
  author = {Chrastil, Elizabeth R. and Warren, William H.},
