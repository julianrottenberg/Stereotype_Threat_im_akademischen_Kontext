  author = {Brady, Shannon T. and Reeves, Stephanie L. and Garcia, Julio and {Purdie-Vaughns}, Valerie and Cook, Jonathan E. and {Taborsky-Barba}, Suzanne and Tomasetti, Sarah and Davis, Eden M. and Cohen, Geoffrey L.},
