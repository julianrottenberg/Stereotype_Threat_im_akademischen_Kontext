@article{nicolasWhenHowStereotype2020a,
  title = {When and How Stereotype Threat Influences Older Adults' Arithmetic Performance: {{Insight}} from a Strategy Approach.},
  shorttitle = {When and How Stereotype Threat Influences Older Adults' Arithmetic Performance},
  author = {Nicolas, Poshita and Lemaire, Patrick and R{\'e}gner, Isabelle},
